{
  "expo": {
    "name": "GroceryGuj",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "groceryguj",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.theamal11qw.groceryguj",
      "buildNumber": "1",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "We need access to your photo library to let you upload profile pictures and product images."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.theamal11qw.groceryguj",
      "versionCode": 1,
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE"
      ],
      "softwareKeyboardLayoutMode": "pan",
      "proguard": {
        "config": "./android/app/proguard-rules.pro"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos to let you upload profile pictures."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "600ee78d-6dee-45ef-ac27-ec73bf65311c"
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "checkAutomatically": "ON_LOAD"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "owner": "theamal11qw"
  }
}
